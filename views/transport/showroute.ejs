<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head.ejs %>

</head>
<body class="nav-md">
<div class="container body">
    <% include ../partials/menuandtopnav.ejs %>

    <div class="right_col" role="main">
        <div class="x_panel">
            <div class="x_title">
                <div class="page-title">
                    <div class="title_left">
                        <h3>Bus Routes</h3>
                    </div>
                    <div class="title_right">
                        <div class="col-md-3 col-sm-3 col-xs-12 form-group pull-right top_search">
                            <button type="button" class="btn btn-round btn-primary"
                                    onclick="location.href='/transport/addroute'">
                                Add Bus-Route
                            </button>
                        </div>
                    </div>
                </div>
            </div>

            <!--Table show start -->
            <table id="datatable-responsive" class="table table-striped table-bordered dt-responsive nowrap"
                   cellspacing="0" width="100%">
                <thead>
                <tr>
                    <th>Route No</th>
                    <th>Registraion Date</th>
                    <th>Insurance Date</th>
                    <th>Fitness Date</th>
                    <th>Registration no</th>
                    <th>Driver</th>
                    <th>Conductor</th>
                    <th>Area</th>
                    <th>Action</th>
                </tr>
                </thead>
                <tbody>
                <!-- Dynamically show data start here-->

                <% for (let item of items) { %>
                <tr role="row">
                    <td><%= item.route_no %></td>
                    <td><%= item.date_registration %></td>
                    <td><%= item.date_insurance %></td>
                    <td><%= item.date_fitness %></td>
                    <td><%= item.registration_no %></td>
                    <td><%= item.driver %></td>
                    <td class=""><%= item.conductor %></td>
                    <td class=""><%= item.area %></td>
                    <td class=" last">
                        <a onclick="alert('Under Development');" id="viewbutton" type="button">View</a>
                    </td>
                </tr>
                <% } %>

                <!-- Dynamically show data end here-->
                </tbody>
            </table>

        </div>
        <!--Table show start -->
    </div>
    <!-- /page content -->
</div>
<% include ../partials/jscripts.ejs %>
</body>
</html>